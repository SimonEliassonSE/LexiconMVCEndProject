@model List<CartItem>
@{
    ViewData["Title"] = "Shoping Cart";

}
<h4 class="mt-3">Shoping Cart</h4>

<table class="table table-striped table-fit table-bordered table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Quantity</th>
            <th>Price</th>
            <th>CartId</th>
            <th>ProductId</th>
          


        </tr>



    <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.CartItemId</td>
                <td>@item.Quantity</td>
                <td>@item.Price</td>
                <td>@item.CartId</td>
                <td>@item.ProductId</td>
             
               
          
            </tr>
            }

    </tbody>
</table>

<a asp-controller="Product" asp-action="Index" class="mt-3 btn btn-primary"> Go back </a>

